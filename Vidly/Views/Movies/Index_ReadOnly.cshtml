@model IEnumerable<Vidly.Models.Movie>

@{
    ViewBag.Title = "Movies";
}

<h2 class="mt-4 mb-4">Movies</h2>

<table id="movies" class="table table-bordered">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                Genere
            </th>
        </tr>
    </thead>

    <tbody>
       
    </tbody>
</table>

@section scripts
{
    <script src="~/Scripts/app/movie_index_readOnly.js"></script>
}

